{
  "name": "@react-native-oh-tpl/react-native-screenshot-prevent",
  "version": "1.2.1-0.0.1",
  "description": "This fork contains fully working blank screenshot on IOS13+ including screen recording",
  "main": "lib/module/index",
  "module": "lib/module/index",
  "types": "lib/typescript/index.d.ts",
  "typings": "lib/typescript/index.d.ts",
  "react-native": "./src/index.ts",
  "source": "./src/index.ts",
  "files": [
    "src",
    "harmony",
    "lib"
  ],
  "scripts": {
    "test": "jest",
    "blob": "bob build",
    "prepare": "npm run blob"
  },
  "publishConfig": { "registry": "https://registry.npmjs.org/", "access": "public" },
  "harmony": {
    "alias": "react-native-screenshot-prevent",
   "codegenConfig": {
      "specPaths": [
        "./src"
      ]
    }
  },
  "keywords": [
    "react-native",
    "screenshot-prevent",
    "android",
    "harmony"
  ],
  "repository": "https://github.com/killserver/react-native-screenshot-prevent",
  "author": "killserver",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/killserver/react-native-screenshot-prevent/issues"
  },
  "homepage": "https://github.com/killserver/react-native-screenshot-prevent#readme",
  "devDependencies": {
    "react-native-builder-bob": "^0.21.2",
    "@commitlint/config-conventional": "^17.0.2",
    "@evilmartians/lefthook": "^1.2.2",
    "@react-native-community/eslint-config": "^3.0.2",
    "@release-it/conventional-changelog": "^5.0.0",
    "@types/jest": "^28.1.2",
    "@types/node": "^20.3.1",
    "@types/react": "~17.0.21",
    "@types/react-native": "0.70.0",
    "del-cli": "^5.0.0",
    "eslint": "^8.4.1",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.0.0",
    "jest": "^28.1.1",
    "pod-install": "^0.1.0",
    "prettier": "^2.0.5",
    "react": "18.2.0",
    "react-native": "0.71.7",
    "typescript": "^4.5.2"
  },
  "resolutions": {
    "@types/react": "17.0.21"
  },
  "peerDependencies": {
    "react": "*",
    "react-native": "*"
  },
  "engines": {
    "node": ">= 16.0.0"
  },
  "packageManager": "yarn@1.22.22+sha256.c17d3797fb9a9115bf375e31bfd30058cac6bc9c3b8807a3d8cb2094794b51ca",
  "jest": {
    "preset": "react-native",
    "modulePathIgnorePatterns": [
      "<rootDir>/lib/"
    ]
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "eslintConfig": {
    "root": true,
    "extends": [
      "@react-native-community",
      "prettier"
    ],
    "rules": {
      "prettier/prettier": [
        "error",
        {
          "quoteProps": "consistent",
          "singleQuote": true,
          "tabWidth": 4,
          "trailingComma": "es5",
          "useTabs": true
        }
      ]
    }
  },
  "eslintIgnore": [
    "node_modules/",
    "lib/"
  ],
  "prettier": {
    "quoteProps": "consistent",
    "singleQuote": true,
    "tabWidth": 4,
    "trailingComma": "es5",
    "useTabs": true
  },
  "codegenConfig": {
    "name": "RNScreenshotPreventSpec",
    "type": "modules",
    "jsSrcsDir": "src"
  },
  "react-native-builder-bob": {
    "source": "src",
    "output": "lib",
    "targets": [
      "commonjs",
      "module"
    ]
  }
}
